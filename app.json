{
    "endpoints": {
        "/log": {
            "get": {
                "js_module": "endpoints/log.js",
                "js_function": "getLogItem",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readonly"
            },
            "post": {
                "js_module": "endpoints/log.js",
                "js_function": "setLogItem",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            }
        },
        "/roles": {
            "get": {
                "js_module": "endpoints/rbac.js",
                "js_function": "getRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readonly"
            },
            "post": {
                "js_module": "endpoints/rbac.js",
                "js_function": "addRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            },
            "delete": {
                "js_module": "endpoints/rbac.js",
                "js_function": "removeRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            },
            "put": {
                "js_module": "endpoints/rbac.js",
                "js_function": "updateRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            }
        },
        "/roleAssignments": {
            "post": {
                "js_module": "endpoints/rbac.js",
                "js_function": "assignPrincipalToRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            },
            "delete": {
                "js_module": "endpoints/rbac.js",
                "js_function": "removePrincipalFromRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readwrite"
            }
        },
        "/checkRole": {
            "get": {
                "js_module": "endpoints/rbac.js",
                "js_function": "isInRbacRole",
                "forwarding_required": "always",
                "authn_policies": [
                    "user_cert"
                ],
                "mode": "readonly"
            }
        }
    }
}